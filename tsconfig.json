{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["astro/client"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020", "DOM"]
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}